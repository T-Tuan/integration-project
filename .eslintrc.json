{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    // "plugin:@typescript-eslint/recommended",
    // "plugin:prettier/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "prettier/prettier": ["error",  {
      "endOfLine": "auto"
    }
  ]
    // "end-of-line": ["error", "auto"]
    // "no-unused-vars": 1
    // "indent": ["error", 6]
    // "linebreak-style": ["error", "windows"],
    // "quotes": ["error", "single"]
    // "semi": ["error", "never"]
  }
}
